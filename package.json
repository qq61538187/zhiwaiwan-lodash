    "less": "^4.1.2",
    "prettier": "3.0.3",
    "qunit-extras": "^3.0.0",
    "qunitjs": "^2.4.1",
    "vitepress": "1.0.0-rc.44",
    "vitepress-theme-demoblock": "^3.0.3"
  },
  "dependencies": {
    "chalk": "4",
    "closure-compiler": "0.2.12",
    "download-git-repo": "^3.0.2",
    "glob": "7.1.1",
    "handlebars": "^4.7.7",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "log-symbols": "4",
    "ora": "5",
    "semver": "5.3.0",
    "uglify-js": "2.7.5"
